// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/bin": true,
    "**/obj": true,
    "**/node_modules": true,
    "**/*.js.map": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/packages": true,
    "*.sln": true,
    "analysis": true,
    "database": true,
    ".gitignore": true,
    "**/project.lock.json": true
  }
}