{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "Program.fs"
      ]
    },
    "copyToOutput": {
      "mappings": {
        "appconfiguration.json": "../BookMate.Core/appconfiguration.json"
      }
    }
  },
  "tools": {
    "dotnet-compile-fsc": "1.0.0-preview2-*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0"
        },
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160629"
      }
    }
  },
  "dependencies": {
    "System.Net.Http": "4.3.0",
    "BookMate.Core": "*",
    "BookMate.Processing": "*",
    "BookMate.Integration": "*"
  },
  "runtimes": {
    "win7-x64": {}
  }
}